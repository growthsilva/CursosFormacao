algoritmo "media-dois"

var
   nome: caractere
   n1, n2 : inteiro
   m : real
inicio
      Escreva ("Digite seu nome:")
      Leia (nome)
      Escreval ("Muito prazer ", nome)
      Escreval ("Informe um numero: ")
      Leia (n1)
      Escreval ("Informe um numero: ")
      Leia (n2)
      m <- (n1 + n2)/2
      Escreva ("A media entre ", n1 ," e ", n2, " é ", m)
fimalgoritmo
